<Page x:Class="ExamenRecu2Ev_SantiPuebla.Views.Pages.ProfilePage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:ExamenRecu2Ev_SantiPuebla.Views.Pages"
      xmlns:ui="http://schemas.lepo.co/wpfui/2022/xaml"
      mc:Ignorable="d"
      d:DataContext="{d:DesignInstance local:ProfilePage,  IsDesignTimeCreatable=True}"
      Title="Perfil"
      d:DesignHeight="450"
      d:DesignWidth="800"
      ui:Design.Background="White">

    <Grid>
        <ui:Button Content="Cerrar Sesión"
                Command="{Binding ViewModel.LogoutCommand}"
                Height="45"
                HorizontalAlignment="Center"
                Appearance="Primary"
                Margin="0,10,0,0"/>
    </Grid>
</Page>
